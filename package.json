{
  "name": "atatus-nodejs-distributed",
  "version": "1.0.0",
  "description": "Distributed Tracing in Node.js with OpenTelemetry and Atatus",
  "main": "index.js",
  "scripts": {
    "payment": "OTEL_EXPORTER_OTLP_ENDPOINT=127.0.0.1:4317 OTEL_RESOURCE_ATTRIBUTES=service.name=PaymentService ts-node-dev -r ./src/tracer.ts ./src/payment-service.ts",
    "customers": "OTEL_EXPORTER_OTLP_ENDPOINT=127.0.0.1:4317 OTEL_RESOURCE_ATTRIBUTES=service.name=CustomerService ts-node-dev -r ./src/tracer.ts ./src/customer-service.ts",
    "orders": "OTEL_EXPORTER_OTLP_ENDPOINT=127.0.0.1:4317 OTEL_RESOURCE_ATTRIBUTES=service.name=OrderService ts-node-dev -r ./src/tracer.ts ./src/order-service.ts"
  },
  "author": "Atatus Team",
  "license": "MIT",
  "dependencies": {
    "@opentelemetry/api": "^1.0.4",
    "@opentelemetry/api-logs": "^0.52.0",
    "@opentelemetry/auto-instrumentations-node": "^0.25.0",
    "@opentelemetry/exporter-collector": "0.25.0",
    "@opentelemetry/exporter-collector-grpc": "^0.25.0",
    "@opentelemetry/exporter-otlp-grpc": "^0.26.0",
    "@opentelemetry/instrumentation-winston": "^0.38.0",
    "@opentelemetry/resources": "^0.24.0",
    "@opentelemetry/sdk-logs": "^0.52.0",
    "@opentelemetry/sdk-node": "0.27.0",
    "@opentelemetry/sdk-trace-base": "^1.0.1",
    "@opentelemetry/sdk-trace-node": "^1.0.1",
    "@opentelemetry/semantic-conventions": "^0.24.0",
    "@opentelemetry/tracing": "^0.24.0",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.17",
    "@types/sequelize": "4.28.11",
    "axios": "^0.25.0",
    "cors": "2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "mysql2": "^2.3.3",
    "sequelize": "^6.16.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  }
}
